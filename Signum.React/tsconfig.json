{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": false,
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "incremental": true,
    "outDir": "./ts_out",
    "composite": true,
    //"noEmit": true,
    "strict": true,
    "declarationMap": true,
    "lib": [
      "es2017",
      //"esnext",
      "dom"
    ]
  },
  "exclude": [
    "./node_modules",
    "./ts_out" 
  ]
}
