<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Model Entity</Title>
      <Shortcut>modelEntity</Shortcut>
      <Description>New ModelEntity</Description>
      <Author>Signum Software</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>entityName</ID>
          <ToolTip>Name of the ModelEntity</ToolTip>
          <Default>Foo</Default>
        </Literal>
        <Literal>
          <ID>Name</ID>
          <ToolTip>Name Property</ToolTip>
          <Default>Name</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp">
        <![CDATA[public class $entityName$Model: ModelEntity
{
    [StringLengthValidator(Min = 3, Max = 100)]
    public string $Name$ { get; set; }
      
    $end$    
}]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
